import java.util.Arrays;
import java.util.Scanner;
class Starting_Point{
    public static void main(String args[])
    {
    String[] a=new String[] {"Apple","Surabhi","Shubham"};
    for(int i=0;i<a.length;i++)
    {
    String name= a[i];
    if(name.charAt(0)=='S')
    System.out.println(name);
    }
    }   
}
