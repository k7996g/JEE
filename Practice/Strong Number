import java.util.Scanner;
class Strong
{ public static void main(String args[])
{Scanner o=new Scanner(System.in);
int n=o.nextInt();
int i,f=1,sum=0,x,j;
x=n;
while(n>0)
{j=n%10;
n=n/10;
for(i=1;i<=j;i++)
{f=f*i;}
sum+=f;
f=1;
}
if(sum==x)
System.out.println("yes");
else
System.out.println("no");
}
}
